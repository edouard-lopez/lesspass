{
  "name": "lesspass",
  "version": "1.0.0",
  "description": "lesspass is like keepass without the need to persist passwords",
  "main": "app/app.js",
  "scripts": {
    "test": "mocha --compilers js:babel-core/register tests",
    "test:watch": "npm run test -- -w",
    "prestart": "npm install",
    "start": "webpack-dev-server --inline --hot --config config/webpack.config.js",
    "prebuild": "rimraf dist && npm prune",
    "build": "webpack --progress --hide-modules --config config/webpack.prod.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/guillaumevincent/lesspass.git"
  },
  "keywords": [
    "keepass",
    "lesspass"
  ],
  "author": "Guillaume Vincent",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/guillaumevincent/lesspass/issues"
  },
  "homepage": "https://github.com/guillaumevincent/lesspass#readme",
  "dependencies": {
    "bootstrap": "^4.0.0-alpha.2",
    "clipboard": "^1.5.5",
    "vue": "^1.0.13",
    "vue-i18n": "^2.3.3"
  },
  "devDependencies": {
    "babel-core": "^6.3.26",
    "babel-loader": "^6.2.1",
    "babel-plugin-transform-runtime": "^6.3.13",
    "babel-preset-es2015": "^6.3.13",
    "babel-runtime": "^5.8.34",
    "css-loader": "^0.23.1",
    "eslint": "^1.10.3",
    "eslint-loader": "^1.2.0",
    "file-loader": "^0.8.5",
    "jshint": "latest",
    "mocha": "latest",
    "node-sass": "^3.4.2",
    "node-static": "latest",
    "nodemon": "latest",
    "npm-run-all": "latest",
    "rimraf": "^2.5.0",
    "template-html-loader": "0.0.3",
    "uglify-js": "^2.6.1",
    "url-loader": "^0.5.7",
    "vue-hot-reload-api": "^1.2.2",
    "vue-html-loader": "^1.1.0",
    "vue-loader": "^8.0.0",
    "vue-style-loader": "^1.0.0",
    "vueify": "^8.2.0",
    "vueify-insert-css": "^1.0.0",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.0"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  }
